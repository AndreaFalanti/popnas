{
    "cnn_hp": {
        "epochs": 600,
        "learning_rate": 0.025,
        "filters": 24,
        "weight_reg": 5e-4,
        "use_adamW": true,
        "drop_path_prob": 0.4,
        "cosine_decay_restart": {
            "enabled": false,
            "period_in_epochs": 2,
            "t_mul": 2.0,
            "m_mul": 1.0,
            "alpha": 0.0
        },
        "softmax_dropout": 0.0
    },
    "architecture_parameters": {
        "motifs": 3,
        "normal_cells_per_motif": 2,
        "block_join_operator": "add",
        "lookback_reshape": false,
        "concat_only_unused_blocks": true,
        "multi_output": true
    }
}